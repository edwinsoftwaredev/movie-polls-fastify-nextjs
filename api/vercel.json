{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/middleware.ts"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-from-middleware",
          "value": "true"
        }
      ],
      "destination": "/api/serverless.ts"
    }
  ]
}
