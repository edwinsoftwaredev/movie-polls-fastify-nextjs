generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  provider      String
  id            String   @id
  displayName   String
  email         String?
  picture       String?
  emailVerified Boolean  @default(false)
  sessions      Session[]
}

model Session {
  id        String  @id
  user      User?   @relation(fields: [userId], references: [id])
  userId    String?
  csrfToken String
}
