{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon api/serverless.ts",
    "build": "prisma generate --schema ../prisma/schema.prisma && prisma migrate deploy --schema ../prisma/schema.prisma && tsc -p tsconfig.json",
    "test": "vitest",
    "lint": "eslint .",
    "syntax-check": "tsc --emitDeclarationOnly false --noEmit true",
    "ci": "eslint . && tsc --emitDeclarationOnly false --noEmit true"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/session": "^10.9.0",
    "@upstash/ratelimit": "^2.0.2",
    "@upstash/redis": "1.34.0",
    "@vercel/edge": "^1.1.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "google-auth-library": "^9.14.1",
    "trpc": "1.0.0",
    "undici": "^6.19.8"
  },
  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "@vercel/node": "^3.2.12",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "@prisma/client": "*",
    "@trpc/server": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "typescript": "*"
  }
}
